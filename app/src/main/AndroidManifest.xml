<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.automaticattendance">

    <application
        android:allowBackup="true"
        android:icon="@drawable/attendance_icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudentRegistration" />
        <activity android:name=".TeacherRegistration" />
        <activity android:name=".CourseList" />
        <activity android:name=".NewCource" />
        <activity android:name=".About" />
        <activity android:name=".TeacherMarks" />
        <activity android:name=".AddStudent" />
        <activity android:name=".SetCode" />
        <activity android:name=".EnterCode" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>